forward FindBadSign(string[]);
public FindBadSign(string[])
{
    for(new i; i < strlen(string); i++)
    {
        switch(string[i])
        {
            case 0x30..0x39: continue; //[0-9]
            case 0x41..0x5A: continue; //[A-Z]
            case 0x61..0x7A: continue; //[a-z]
            case '_', '[', ']', '(', ')': continue;
            default: return 1;
        }
    }
    return 0;
}
