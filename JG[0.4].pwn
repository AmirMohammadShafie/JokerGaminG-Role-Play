    }
    }
    }
    else
    {
    if(Specing[playerid] == 1)
    {
    if(Specing2[playerid] != 1)
    {
    PlayerSpectatePlayer(playerid,OnSpecPlayer[playerid]);
    Specing[playerid] = 1;
    Specing2[playerid] = 1;
    }
