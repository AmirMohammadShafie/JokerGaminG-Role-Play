    PlayerSpectatePlayer(playerid,OnSpecPlayer[playerid]);
    Specing[playerid] = 1;
    Specing2[playerid] = 1;
    }
    }
    }
    }
    }
    else
    {
    SCM(playerid,COLOR_Good,"Player left the game.");
    StopSpec(playerid);
    }
    }
    return 1;
}

